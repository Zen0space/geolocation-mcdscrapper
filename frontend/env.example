# Development Configuration
NODE_ENV=development

# Bundle Analysis (set to 'true' to analyze bundle)
ANALYZE=false 


# IMPORTANT: Use https:// URL format, NOT libsql:// for remote Turso databases
TURSO_DATABASE_URL=https:.....
TURSO_AUTH_TOKEN=your_turso_auth_token_here


GEOCODING_PROVIDER=nominatim


# Google Gemini 2.5 Flash (free tier)
GEMINI_API_KEY=your_gemini_api_key_here
CHATBOT_PROVIDER=gemini


DEBUG=true
LOG_LEVEL=info

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_MAP_PROVIDER=react-leaflet

# Optional: Free map tile providers
NEXT_PUBLIC_MAP_TILES=openstreetmap